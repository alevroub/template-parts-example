<%
	const { request } = it;
	const url = new URL(request.url);
%>

<div>>>>page.eta</div>

<div>
	<span>Navigated to: </span>
	<span><%= JSON.stringify(request.url, null, 3) %></span>
</div>

<div>
	<% for (const parameter of url.searchParams.entries()) {%>
		<div><%= parameter %></div>
	<% } %>
</div>

<div>
	<span>Data received from controller: </span>
	<pre><%= JSON.stringify(it.data, null, 3) %></pre>
</div>